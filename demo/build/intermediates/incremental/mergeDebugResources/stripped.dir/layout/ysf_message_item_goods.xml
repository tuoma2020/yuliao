<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/ysf_bubble_max_width"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical">

    <include
        android:id="@+id/ysf_goods_content"
        layout="@layout/ysf_message_item_goods_inner" />
    <View
        android:id="@+id/ysf_holder_product_list_line"
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="10dp"
        android:background="@color/ysf_grey_e6e6e6"/>
    <TextView
        android:id="@+id/tv_ysf_item_message_goods"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="重新选择"
        android:paddingTop="10dp"
        android:paddingBottom="5dp"
        android:textColor="@color/ysf_blue_5e94e2"
        android:layout_gravity="center_horizontal"
        android:textSize="14sp"/>
</LinearLayout>